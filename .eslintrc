{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "cypress/globals": true
  },
  "extends": [
    "airbnb",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "max-len": [1, 200],
    "no-shadow": 0,
    "no-plusplus": 0,
    "import/no-unresolved": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": [
      1,
      {
        "skipUndeclared": true
      }
    ]
  }
}